{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "html", 
    "import",
    "node",
    "promise",
    "prettier"
  ],
  "rules": {}
}
